<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>KualiDocument</name>
      <description>KualiDocument</description>
      <label>KualiDocument</label>
      <postProcessorName>org.kuali.rice.krad.workflow.postprocessor.KualiPostProcessor</postProcessorName>
      <active>true</active>
      <policies>
        <policy>
          <name>DEFAULT_APPROVE</name>
          <value>true</value>
        </policy>
        <policy>
          <name>LOOK_FUTURE</name>
          <value>true</value>
        </policy>
      </policies>
      <routingVersion>2</routingVersion>
      <routePaths>
        <routePath>
          <start name="PreRoute" />
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="PreRoute">
          <activationType>S</activationType>
          <mandatoryRoute>false</mandatoryRoute>
          <finalApproval>false</finalApproval>
        </start>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>

